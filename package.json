{
  "devDependencies": {
    "browser-sync": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.4.17"
  },
  "scripts": {
    "browser-sync": "browser-sync start --config browser-sync-config.js",
    "watch:css": "npx tailwindcss -i ./src/styles/tailwind.css -o ./auctions/static/auctions/styles.css --watch",
    "dev": "npm-run-all --parallel watch:css browser-sync"
  }
}
